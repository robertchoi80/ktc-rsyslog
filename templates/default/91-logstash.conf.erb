$MaxMessageSize 64k
$WorkDirectory /var/spool/rsyslog
$ActionQueueType <%= node['rsyslog']['queue']['type'] %>
$ActionQueueSize 200000
$ActionQueueFileName <%= node['rsyslog']['queue']['file_name'] %>
$ActionQueueMaxFileSize 1g
$ActionQueueMaxDiskSpace <%= node['rsyslog']['queue']['max_disk_space'] %>
$ActionQueueLowWatermark 100000
$ActionQueueHighWatermark 150000
$ActionQueueDiscardMark 150000
$ActionQueueDiscardSeverity 8
$ActionQueueSaveOnShutdown on
$ActionQueueWorkerThreads 1

<% case @protocol -%>
<% when "tcp" -%>
*.*;auth,authpriv.none    @@<%= @server %>:<%= node['rsyslog']['logstash_server_port'] %>
